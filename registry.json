{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "abui",
  "homepage": "https://abui.antoniobrandao.com",
  "items": [
    {
      "name": "design-system",
      "type": "registry:style",
      "dependencies": ["radix-ui"],
      "registryDependencies": [
        "@abui/button",
        "@abui/input",
        "@abui/calendar",
        "@abui/card",
        "@abui/label",
        "@abui/popover",
        "@abui/textarea"
      ],
      "cssVars": {
        "light": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)",
          "radius": "0.825rem"
        },
        "dark": {
          "primary": "oklch(0.63 0.17 149)",
          "primary-foreground": "oklch(0.985 0 0)"
        }
      },
      "files": [
        {
          "path": "registry/abui/rules/AGENTS.md",
          "type": "registry:file",
          "target": "~/AGENTS.md"
        }
      ]
    },
    {
      "name": "button",
      "type": "registry:ui",
      "title": "Button",
      "description": "A button component.",
