{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "site-header",
  "type": "registry:block",
  "title": "Site Header",
  "description": "A site header component.",
  "registryDependencies": [
    "@abui/logo",
    "@abui/main-nav"
  ],
  "files": [
    {
      "path": "registry/alpine/navigation/site-header.tsx",
      "content": "import { Logo } from \"@/registry/abui/branding/logo\"\nimport { MainNav } from \"@/registry/abui/navigation/main-nav\"\nimport { Button } from \"@/registry/abui/ui/button\"\nimport Link from \"next/link\"\n\nexport function SiteHeader() {\n  return (\n    <header className=\"sticky top-0 z-50 w-full border-b bg-background\">\n      <div className=\"container flex h-16 items-center mx-auto px-4 gap-4\">\n        <Link href=\"/\">\n          <Logo /> <span className=\"sr-only\">Abui</span>\n        </Link>\n        <MainNav />\n        <div className=\"ml-auto flex items-center gap-4\">\n          <Button>Get Started</Button>\n        </div>\n      </div>\n    </header>\n  )\n}\n",
      "type": "registry:component"
    }
  ]
}